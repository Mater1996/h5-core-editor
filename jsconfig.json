{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "luban-h5": ["packages/luban-h5/src"],
      "@luban-h5/*": ["packages/*/src"],
    }
  },
  "include": ["packages/*/src", "packages/*/__tests__"],
  "exclude": ["dist", "node_modules", "example"]
}
