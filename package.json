{
  "name": "luban-h5-editor",
  "version": "0.0.31",
  "scripts": {
    "dev": "npm run dev:editor",
    "build": "npm run build:editor & npm run build:preview",
    "dev:editor": "rollup -c ./build/rollup.config.js -w",
    "build:editor": "rollup -c ./build/rollup.config.js --prod",
    "dev:preview": "rollup -c ./build/rollup.preview.config.js -w",
    "build:preview": "rollup -c ./build/rollup.preview.config.js --prod",
    "build:analyze": "rollup -c ./build/rollup.config.js --prod --analyze",
    "build:example": "npm run --prefix ./example build",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue ./src && npm run lint:style",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx,.vue ./src",
    "lint:style": "stylelint --fix \"./src/**/*.scss\" --syntax scss",
    "prettier": "prettier -c --write \"**/*\""
  },
  "main": "dist/luban-h5-editor.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "animate.css": "^3.7.2",
    "ant-design-vue": "^1.7.0",
    "core-js": "^3.6.5",
    "echarts": "^4.8.0",
    "font-awesome": "4.7.0",
    "hotkeys-js": "^3.7.6",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.20",
    "papaparse": "^5.2.0",
    "qrcode": "^1.4.1",
    "resize-detector": "^0.2.2",
    "v-charts": "^1.19.0",
    "vant": "^2.10.11",
    "vue": "^2.6.10",
    "vue-i18n": "^8.14.1",
    "vue-quill-editor": "^3.0.6",
    "x-data-spreadsheet": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/babel-preset-app": "^4.5.8",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/eslint-config-standard": "^5.1.2",
    "autoprefixer": "^9.0.0",
    "babel-plugin-import": "^1.13.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.14.1",
    "postcss-url": "^9.0.0",
    "rollup": "^2.32.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.18.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.12"
  }
}
